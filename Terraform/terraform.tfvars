# VPC CIDR block
vpc_cidr_block = "10.0.0.0/16"

# Subnet CIDR block
subnet_1_cidr_block = "10.0.10.0/24"

# Availability zone
avail_zone = "eu-north-1a"  # change if needed

# Environment prefix
env_prefix = "dev"

# EC2 instance type
instance_type = "t3.micro"

# SSH public key (for EC2 user)
ssh_key = "~/.ssh/id_rsa.pub"  

# SSH private key path (sensitive)
ssh_key_private = "~/.ssh/id_rsa" 